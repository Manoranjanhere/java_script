let Bhero=Boolean(hero);
console.log(Bhero)
console.log(typeof Bhero);
//"mano"=>true;
//""=>false;
let a =33
let s=String(a);
console.log(s);
console.log(typeof s);
//operations
console.log("1"+2+3);//123
console.log(1+2+"3");//33
//conversion from left to right;
console.log(+true);
console.log(+"");
console.log(null>0);
console.log(null==0);
console.log(null>=0);
console.log("2"==2);
console.log("2"===2);
const id=Symbol('123')
const ide=Symbol('123')
console.log(id);
console.log(ide);
console.log(id==ide)
